{
  "name": "phonenum-generator",
  "version": "1.0.0",
  "description": "an api to generate random phone numbers",
  "main": "./dist/app.js",
  "repository": "https://github.com/rickynyairo/phonenum-generator",
  "author": "rickynyairo <rickynyairo@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/app.js\"",
    "start:node": "yarn build && node ./dist/app.js -d",
    "lint": "tslint 'src/**/*.ts' --fix -p tsconfig.json -c tslint.json",
    "test": "export NODE_ENV=testing && jest --coverage --forceExit --detectOpenHandles",
    "start": "pm2 start ecosystem.config.js --env production",
    "test:coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">=10.0.0 <=11.4.0",
    "npm": ">=6.0.0 <=7.0.0 "
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cassandra-store": "^5.0.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "coveralls": "^3.0.5",
    "dotenv": "^8.0.0",
    "envalid": "^5.0.0",
    "express": "5.0.0-alpha.7",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.17",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.5.9",
    "@types/node": "^12.6.2",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1"
  }
}
